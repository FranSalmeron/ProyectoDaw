{% extends 'base.html.twig' %}

{% block title %}CarFavorite index{% endblock %}

{% block body %}
    <h1>CarFavorite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for car_favorite in car_favorites %}
            <tr>
                <td>{{ car_favorite.id }}</td>
                <td>
                    <a href="{{ path('app_car_favorite_show', {'id': car_favorite.id}) }}">show</a>
                    <a href="{{ path('app_car_favorite_edit', {'id': car_favorite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_car_favorite_new') }}">Create new</a>
{% endblock %}
